<?xml version="1.0" encoding="UTF-8" ?>
<templates id="g2p_additional_info" xml:space="preserve">
    <t t-name="g2p.addl.info.template">
        <table class="o_group">
            <tbody>
                <t t-foreach="value" t-as="key">
                    <tr>
                        <td class="o_td_label" colspan="2" style="width: 40%;">
                            <label class="o_form_label o_quick_editable"><t t-esc="key" /></label>
                        </td>
                        <td colspan="3" style="width: 100%;">
                            <span class="o_field_widget o_quick_editable"><t t-esc="value[key]" /></span>
                        </td>
                    </tr>
                </t>
            </tbody>
        </table>
    </t>
</templates>
